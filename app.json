{
  "expo": {
    "name": "cashew_mobile",
    "slug": "cashew_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_color.svg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_color.svg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
